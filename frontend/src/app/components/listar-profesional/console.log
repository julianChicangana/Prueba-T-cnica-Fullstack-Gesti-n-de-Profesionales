this.profesionalService.getProfesionales().subscribe({
  next: (data) => {
    console.log('Datos de la API:', data);
    this.profesionales = data;
  },
  error: (err) => console.error('Error al obtener profesionales:', err)
});

editar(id_persona: number): void {
  console.log('ID recibido para editar:', id_persona);
  if (id_persona === undefined || id_persona === null) {
    console.error('ID inv√°lido:', id_persona);
    return;
  }
  this.router.navigate(['/editar', id_persona]);
}
